{
  "private": true,
  "name": "firefly-test",
  "version": "1.0.0",
  "description": "Front-end coding test",
  "main": "app/index.js",
  "scripts": {
    "clean-dist": "node ./node_modules/.bin/rimraf dist",
    "copy-files": "cp app/index.html dist/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "e2e": "node ./node_modules/.bin/protractor test/protractor.conf.js",
    "webdriver-start": "node ./node_modules/.bin/webdriver-manager start",
    "webdriver-update": "node ./node_modules/.bin/webdriver-manager update",
    "start": "node node_modules/.bin/webpack-dev-server --content-base app",
    "webpack-validate-dev": "NODE_ENV=development node node_modules/.bin/webpack-validator webpack.config.js",
    "webpack-validate-prod": "NODE_ENV=production node node_modules/.bin/webpack-validator webpack.config.js",
    "build": "npm run clean-dist && NODE_ENV=production node node_modules/.bin/webpack && npm run copy-files",
    "deploy": "git push heroku tests:master && heroku open"
  },
  "author": "Joan Saez",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "^4.3.0",
    "protractor": "^4.0.14",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-core": "^0.6.9",
    "webpack-dev-server": "^1.16.2",
    "webpack-validator": "^2.3.0"
  },
  "dependencies": {
    "angular": "^1.6.1",
    "angular-ui-bootstrap": "^2.4.0",
    "angular-ui-router": "^0.3.2",
    "bootstrap": "^3.3.7",
    "express": "^4.14.0",
    "gzippo": "^0.2.0",
    "jquery": "^3.1.1",
    "morgan": "^1.7.0"
  }
}
